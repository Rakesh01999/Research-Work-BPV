<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">

    <input>
        <net-file value="Test1.net.xml"/>
        <route-files value="Test1.rou.xml"/>
        <additional-files value="Test1.add.xml"/>
    </input>

    <time>
        <begin value="0"/>
        <!-- <end value="100"/> -->
        <end value="200"/>
        <step-length value="0.5"/>
    </time>

    <processing>
        <collision.check-junctions value="true"/>
        <ignore-route-errors value="true"/>
        <lateral-resolution value="0.8"/>
        <ignore-junction-blocker value="5"/>
        <device.battery.probability value="1"/>
        <device.emissions.probability value="1"/>
    </processing>

    <output>
        <tripinfo-output value="tripinfo.xml"/>
        <fcd-output value="fcd.xml"/>
        <summary-output value="summary.xml"/>
        <chargingstations-output value="chargingstations.xml"/>
        <battery-output value="battery.xml"/>
        <emission-output value="emissions.xml"/>
    </output>

    <report>
        <verbose value="true"/>
        <duration-log.statistics value="true"/>
    </report>

</configuration>