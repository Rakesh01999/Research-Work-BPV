<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

    <!-- ELECTRIC VEHICLE TYPES (Bangladesh Context) -->
    
    <vType id="easyBike" accel="1.2" decel="4.5" sigma="0.8" 
           length="2.2" width="0.8" height="1.4" maxSpeed="19.44" color="0.9,0.3,0.9" 
           minGap="0.5" tau="0.9" speedFactor="1.05" 
           emissionClass="Energy/unknown"/>
    
    <vType id="eRickshaw" accel="0.6" decel="2.5" sigma="0.7" 
           length="2.5" width="1.0" height="1.8" maxSpeed="6.94" color="0,0.9,0" 
           minGap="0.8" tau="2.0" speedFactor="0.8" 
           emissionClass="Energy/unknown"/>
    
    <vType id="eVan" accel="0.7" decel="3.2" sigma="0.6" 
           length="2.5" width="2.0" height="2.2" maxSpeed="13.89" color="0.2,0.6,0.9" 
           minGap="2.0" tau="1.8" speedFactor="0.85" 
           emissionClass="Energy/unknown"/>

    <!-- ALL POSSIBLE ROUTES FROM N1 TO N13 (COMPREHENSIVE) -->
    
    <!-- Route 1: DIRECT - N1→N2→N3→N4→N13 (Shortest) -->
    <route id="route1_direct" edges="E0 E1 E2 E3"/>
    
    <!-- Route 2: VIA N5-N6 - N1→N5→N6→N2→N3→N4→N13 -->
    <route id="route2_via_n5n6" edges="E4 E11 E10 E1 E2 E3"/>
    
    <!-- Route 3: SOUTHERN - N1→N5→N7→N8→N12→N13 -->
    <route id="route3_southern" edges="E4 E17 E18 E14 E15"/>
    
    <!-- Route 4: VIA N5-N7-N8-N9-N10-N6 - N1→N5→N7→N8→N9→N10→N6→N2→N3→N4→N13 -->
    <route id="route4_long_via_n9n10" edges="E4 E17 E18 E7 E8 E9 E10 E1 E2 E3"/>
    
    <!-- Route 5: VIA N11 - N1→N2→N3→N11→N12→N13 -->
    <route id="route5_via_n11" edges="E0 E1 E13 E16 E15"/>
    
    <!-- Route 6: VIA N10-N11 - N1→N5→N6→N10→N11→N12→N13 -->
    <route id="route6_via_n10n11" edges="E4 E11 E9 E12 E16 E15"/>
    
    <!-- Route 7: VIA N9-N10-N11 - N1→N5→N7→N8→N9→N10→N11→N12→N13 -->
    <route id="route7_via_n9n10n11" edges="E4 E17 E18 E7 E8 E12 E16 E15"/>
    
    <!-- Route 8: BACKTRACK VIA N6-N10 - N1→N2→N6→N10→N11→N3→N4→N13 -->
    <route id="route8_backtrack_n6n10" edges="E0 E10 E9 E12 E13 E2 E3"/>

    <!-- INITIAL VEHICLES ON DIFFERENT ROUTES -->
    
    <!-- Easy Bikes (Testing all routes) -->
    <vehicle id="easybike_01" type="easyBike" route="route1_direct" depart="0"/>
    <vehicle id="easybike_02" type="easyBike" route="route2_via_n5n6" depart="2"/>
    <vehicle id="easybike_03" type="easyBike" route="route3_southern" depart="4"/>
    <vehicle id="easybike_04" type="easyBike" route="route4_long_via_n9n10" depart="6"/>
    <vehicle id="easybike_05" type="easyBike" route="route5_via_n11" depart="8"/>
    <vehicle id="easybike_06" type="easyBike" route="route6_via_n10n11" depart="10"/>
    <vehicle id="easybike_07" type="easyBike" route="route7_via_n9n10n11" depart="12"/>
    <vehicle id="easybike_08" type="easyBike" route="route8_backtrack_n6n10" depart="14"/>
    
    <!-- E-Rickshaws on various routes -->
    <vehicle id="erickshaw_01" type="eRickshaw" route="route1_direct" depart="5"/>
    <vehicle id="erickshaw_02" type="eRickshaw" route="route2_via_n5n6" depart="10"/>
    <vehicle id="erickshaw_03" type="eRickshaw" route="route3_southern" depart="15"/>
    <vehicle id="erickshaw_04" type="eRickshaw" route="route4_long_via_n9n10" depart="20"/>
    <vehicle id="erickshaw_05" type="eRickshaw" route="route5_via_n11" depart="25"/>
    
    <!-- Electric Vans on main routes -->
    <vehicle id="evan_01" type="eVan" route="route1_direct" depart="8"/>
    <vehicle id="evan_02" type="eVan" route="route3_southern" depart="18"/>
    <vehicle id="evan_03" type="eVan" route="route5_via_n11" depart="28"/>

